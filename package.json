    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "^1.0.0",
    "xo": "^0.17.1"
  },
  "ava": {
    "files": [
      "test/specs/*.js"
    ]
  },
  "xo": {
    "env": [
      "node",
      "browser"
    ],
    "files": [
      "example/**/*.js",
      "src/**/*.js"
    ],
    "esnext": true,
    "space": 2,
    "ignores": [
      "es/**"
    ],
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": "test/fixtures/*js",
        "esnext": false,
        "globals": [
          "tuta"
        ]
      }
    ]
  },
  "dependencies": {
    "raf": "^3.3.0"
  }
}
