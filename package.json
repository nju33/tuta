  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "ava": "^0.17.0",
    "babel-eslint": "6",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^0.0.9",
    "babili": "^0.0.9",
    "browser-sync": "^2.18.2",
    "casual": "^1.5.11",
    "cssnano": "^3.8.1",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "gulp-postcss": "^6.2.0",
    "nightmare": "^2.8.1",
    "postcss-easings": "^0.3.0",
